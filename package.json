{
  "name": "turborepo-nextjs-trpc-starter",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "dotenv -- npx -w @repo/db prisma db push",
    "db:studio": "dotenv -- npx -w @repo/db prisma studio --port 5556",
    "prod": "dotenv -v NODE_ENV=production turbo build"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "prettier": "^3.1.1",
    "dotenv-cli": "^7.3.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
  }
}
